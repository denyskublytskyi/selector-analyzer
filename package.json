    "imagemin-pngquant": "^9.0.0",
    "lodash": "^4.17.19",
    "mongoose": "^5.9.24",
    "puppeteer-core": "4.0.1",
    "telegraf": "^3.38.0"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.0",
    "prettier": "^2.0.5",
    "puppeteer": "^5.2.1"
  },
  "jest": {
    "verbose": true
  }
